        <activity
            android:name="com.emar.escore.recommendwall.ShowWallRecommendActivity"
            android:configChanges="keyboardHidden|orientation" />
         <!-- 注册墙Activity 九宫格-->
        <activity
            android:name="com.emar.escore.ninewall.ShowWallNineActivity"
            android:configChanges="keyboardHidden|orientation" />
      
